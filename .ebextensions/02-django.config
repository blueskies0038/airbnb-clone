container_commands:
    01_migrate:
        command: "django-admin migrate"
        leader_only: true
    02_createsu:
        command: "django-admin createsu"
    03_seedrooms:
        command: "python manage.py seed_rooms --number 10"
    04_seedreviews:
        command: "python manage.py seed_reviews --number 30"
option_settings:
    aws:elasticbeanstalk:container:python:
        WSGIPath: hosuto/wsgi.py
    aws:elasticbeanstalk:application:environment:
        DJANGO_SETTINGS_MODULE: hosuto.settings